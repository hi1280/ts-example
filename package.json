{
  "name": "qiita-cli",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "mocha -r espower-typescript/guess test/**/*.ts",
    "coverage": "nyc -i ts-node/register --temp-directory coverage/.nyc -r text -r html -n test/**/*.ts -n src/**/*.ts -e .ts mocha test/**/*.ts",
    "build": "tsc",
    "lint": "tslint -p tsconfig.json --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "npm run lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.11",
    "espower-typescript": "^9.0.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "power-assert": "^1.6.1",
    "prettier": "^1.15.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  },
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "qiita-cli": "dist/main.js"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/elasticsearch": "^5.0.29",
    "@types/express": "^4.16.0",
    "@types/helmet": "0.0.42",
    "@types/redis": "^2.8.8",
    "@types/sinon": "^5.0.7",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "elasticsearch": "^15.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "redis": "^2.8.0",
    "sinon": "^7.1.1"
  }
}
